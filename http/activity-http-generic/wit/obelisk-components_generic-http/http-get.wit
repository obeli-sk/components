package obelisk-components:generic-http@0.1.0;

interface http-get {

    record response {
        body: list<u8>,
        status-code: u16,
    }

    get: func(url: string) -> result<string, string>;
    get-resp: func(url: string) -> result<response, string>;
    get-successful: func(url: string) -> result<string, string>;
}
